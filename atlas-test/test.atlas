// Ackermann function
let ackermann: (m: i64, n: i64) -> i64 =
    if m == 0 then
        n + 1
    else
        if m > 0 then
            if n == 0 then
                ackermann(m - 1, 1)
            else 
                if n > 0 then
                    ackermann(m - 1, ackermann(m, n - 1))
                else
                    n + 1
            else n + 1